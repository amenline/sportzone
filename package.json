{
  "name": "sportzone",
  "version": "1.0.0",
  "description": "A sport(mostly soccer and basket ball) blog",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amenline/sportzone.git"
  },
  "author": "Ron Mens",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/amenline/sportzone/issues"
  },
  "homepage": "https://github.com/amenline/sportzone#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "concurrently": "^5.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.6"
  },
  "devDependencies": {
    "nodemon": "^1.19.4"
  }
}
